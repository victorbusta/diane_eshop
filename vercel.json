{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm i && npx prisma generate && npm run build",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/src/main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
